$("#s_submit_eq").click(function(){
		$('#manageeq').show();
})


function optionCheck5(){
		var option4 = $('#opsearch5 option:selected').val();
		switch (option4) {
			case "agregarm":
				
				//$("#agregau").hide();
				$("#agregamarca").show();
				$("#eq_submit").show();
				$("#modelos").hide();
				$("#a_marca").show();
				$("#mod_submit").hide();
				
				
			break;
			case "agregarmm":
			
				$("#mod_submit").show();
				$("#eq_submit").hide();
				$("#agregamarca").show();
				$("#a_marca").hide();
				$("#modelos").show();
			break;

		}

}




$('#eq_submit').click(function(){
	var marca = $('#a_marca').val();
	//var modelo = $('#modelos').val();

	$.ajax({
		type:"GET",
		url: "ins.php?eqsubmit=1&a_marca="+marca,
		dataType:"html",
		success: function(resp){
			$('#agregamarcas').html(resp);
		}
	});
	$('#a_marca').val('');
});

$('#mod_submit').click(function(){
	
	var modelo = $('#modelos').val();

	$.ajax({
		type:"GET",
		url: "ins.php?modsubmit=1&modelos="+modelo,
		dataType:"html",
		success: function(resp){
			$('#agregamarcas').html(resp);
		}
	});
	$('#modelos').val('');
});








