##### Cambios  29 oct 2015 ######


########### base de datos ###############



alter table cliente add RFC varchar(20) after telefono;
alter table cliente add domicilio varchar(30) after RFC;
alter table cliente add ciudad varchar(30) after domicilio;
alter table cliente add colonia varchar(30) after domicilio;
alter table cliente add cpostal varchar(30) after ciudad;
alter table dispos add marca varchar(20) after recibe;
alter table dispos add access varchar(80) after marca;
alter table cliente  modify cpostal int (5);
create table marcas (id int not null auto_increment primary key, marca varchar(25));
create table modelos (id int not null auto_increment primary key, modelo varchar(25));















